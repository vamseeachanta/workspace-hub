{
  "name": "baseline-tracking-engine",
  "version": "1.0.0",
  "description": "Core baseline tracking and comparison engine for test metrics, coverage, and performance data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "node tests/scripts/run-all-tests.js",
    "test:unit": "jest --config tests/jest.config.js tests/unit",
    "test:integration": "jest --config tests/jest.config.js tests/integration --runInBand",
    "test:e2e": "playwright test --config tests/playwright.config.js",
    "test:performance": "jest --config tests/jest.config.js tests/performance --runInBand --detectOpenHandles",
    "test:property": "jest --config tests/jest.config.js tests/property-based",
    "test:mutation": "stryker run tests/stryker.conf.js",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:coverage-verify": "jest --config tests/jest.config.js tests/coverage",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:ci": "node tests/scripts/run-all-tests.js --skip-slow --no-mutation",
    "test:quick": "npm run test:unit && npm run test:integration",
    "test:parallel": "node tests/scripts/run-all-tests.js --parallel",
    "test:mutation-quick": "stryker run tests/stryker.conf.js --mutate 'src/core/**/*.js'",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config tests/jest.config.js --runInBand",
    "test:analyze": "node tests/scripts/analyze-results.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "coverage:open": "open coverage/lcov-report/index.html",
    "mutation:open": "open reports/mutation/index.html",
    "reports:clean": "rm -rf coverage reports",
    "reports:serve": "npx http-server reports -p 8080"
  },
  "keywords": [
    "baseline",
    "tracking",
    "metrics",
    "testing",
    "coverage",
    "performance",
    "comparison",
    "typescript"
  ],
  "author": "Baseline Tracking Engine",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.28.0",
    "@stryker-mutator/core": "^6.2.0",
    "@stryker-mutator/jest-runner": "^6.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.3.0",
    "chalk": "^4.1.2",
    "eslint": "^8.0.0",
    "fast-check": "^3.4.0",
    "glob": "^8.0.3",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "nock": "^13.2.9",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "fs-extra": "^11.1.0",
    "js-yaml": "^4.1.0",
    "semver": "^7.5.0"
  }
}
