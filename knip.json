{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/index.ts",
    "src/main.ts",
    "src/app.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx,js,jsx}",
    "!src/**/*.{test,spec}.{ts,tsx,js,jsx}"
  ],
  "ignore": [
    "dist/**",
    "build/**",
    "node_modules/**",
    "coverage/**",
    "**/*.d.ts"
  ],
  "ignoreDependencies": [],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false,
  "rules": {
    "files": "error",
    "dependencies": "error",
    "devDependencies": "error",
    "unlisted": "error",
    "exports": "warn",
    "types": "warn",
    "nsExports": "warn",
    "nsTypes": "warn",
    "duplicates": "error"
  }
}
