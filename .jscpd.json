{
  "threshold": 5,
  "reporters": ["html", "markdown", "console"],
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/*.min.js",
    "**/*.bundle.js",
    "**/*.test.js",
    "**/*.test.ts",
    "**/*.spec.js",
    "**/*.spec.ts"
  ],
  "format": ["typescript", "javascript", "jsx", "tsx", "python"],
  "minLines": 5,
  "minTokens": 50,
  "output": ".refactor-reports/jscpd",
  "silent": false,
  "blame": false,
  "gitignore": true
}
